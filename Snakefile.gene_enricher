
rule hgnc_complete_set_json:
	input:
		"source/gene_enricher/version.txt"
	output:
		"source/gene_enricher/hgnc_complete_set.json"
	shell:		"curl --verbose --progress-bar --ipv4 --connect-timeout 8 --max-time 120 --retry 128 --ftp-ssl --disable-epsv --ftp-pasv ftp://ftp.ebi.ac.uk/pub/databases/genenames/new/json/hgnc_complete_set.json --output source/gene_enricher/hgnc_complete_set.json"
